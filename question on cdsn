津津的预算
#include <stdio.h>
#include <stdlib.h>
int main() {
    srand((unsigned int)time(0));
    
    for (int i = 0; i < 12; i++)
    {
        int fixed = 300;
        int ep = rand() % (350 - 0 + 1) + 0;
        int left1 = fixed - ep;
        if (left1 > 100)
        {
            int left = left1 % 100;
            int save = left1 / 100;
            left1 = left + fixed;
            printf("%d\n", left);
        }
        else
        {
            int left = left1;
            printf("%d\n", left);
        }
    
        printf("%d\n", ep);
        
            
        }
   
    return 0;
}

取石头问题
//数石头
#include<stdio.h>
#include <stdlib.h>
#include<time.h>
int main()
{
	srand((unsigned int)time(0));
	int i = 0;//每堆石头中石头的个数
	printf("每堆中石头的个数（每堆不超过100000000）：\n");
	int arr[10000] = { 0 };
	while (1)
	{
		scanf_s("%d", &arr[i]);//输入数组
		i++;//计算输入数据的个数
		if (getchar() == '\n')//利用回车跳出循环 解决了输入问题  ：） 但若不输入数据后回车会出错
		{
			break;
		}
	}
//开始取石头
    int get = 0;
	for (int t = 0; t < i; t++)
	{
		int take = rand() % arr[t] + 1;
		int sum = 0;
		for (int g = 0; sum <arr[t]; g++)
		{
			sum = sum + take;
			while (sum < arr[t])
			{
				int take = rand() % (arr[t] - 0 + 1) - 0;
				g++;
			}
			get = g; 
		}
	}
	printf("%d", get);//取石头次数
	int res = get % 2;
	if (res != 0)
		printf("YES");
	else
		printf("NO");
	return 0;
}
计算阶乘
#include<stdio.h>
int sum=1;
int nor=0;
int solution(int N)
{
for(int i=2;i<=N;i++)
{
    nor =i;
    sum=sum^nor
}
}
int main ()
{
      scanf_s(“%d”,&N);
      int fs=solution(N);
      printf(“%d\n”,fs);
      return 0;
}
