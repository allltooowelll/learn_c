#include<stdio.h>
#include<math.h>
int main()
{
    int n;
    scanf("%d",&n);
    int i;
    //if(n!=0){
    double sum=0.0;
    for(i=1;i<=n;i++)
    {
        sum+=sqrt((double)i);
    }
    printf("sum = %.2lf\n",sum);
    //}
    return 0;
}
