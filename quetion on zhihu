}
4.
#include<stdio.h>
int main()
{
	int h = 100;
	int a = 10;
	int t = 3;
	int s = 0.5 * a * t * t;
	if (s < 100)
		printf("height = %d\n", s);
	else
		printf("height=100");
	return 0;
}
5.
#include<stdio.h>
int main()
{
	int F = 0;
	scanf_s("%d", &F);//不要随意在取址函数内部加\n
	int C = 5 * (F - 32) / 9;
	printf("celsius = %d\n", C);
	return 0;
}
6.
#include<stdio.h>
int main()
{
	int x = 0;
	int y = 0;
	scanf_s("%d %d", &x, &y);
	if (y != 0)
	{
		printf("%d+%d=%d\n", x, y, x + y);
		printf("%d-%d=%d\n", x, y, x - y);
		printf("%d*%d=%d\n", x, y, x * y);
		printf("%d/%d=%d\n", x, y, x / y);
	}
	return 0;
}
7.
#include<stdio.h>
float function(float x)
{

	if (x != 0)
		return 1.0 / x;
	else
		return 0;
}
int main()
{
	float a = 0;
	scanf_s("%f", &a);
	float f = function(a);
	printf("f(%.1f)=%.1f", a,f);
	return 0;
}
8.
#include<stdio.h>
#include<math.h>
float function(float x)
{

	if (x >= 0)
		return sqrt(x);
	else
		return (x+1)*(x+1)+2*x+1.0/x;
}
int main()
{
	float a = 0;
	scanf_s("%f", &a);
	float f = function(a);
	printf("f(%.2f)=%.2f", a,f);
	return 0;
}
9.
#include<stdio.h>
int main()
{
	printf("在一行中输入2个整数，分别表示lower和upper的值，中间用空格分开。\n");
	int l = 0;
	int u = 0;
	scanf_s("%d %d",&l, &u);
	if (l < u && u < 100)
	{
		float C1 = 5*(l - 32)/ 9.0;
		float C2 = 5*(u - 32)/ 9.0;
		printf("fahr celsius\n");
		printf("%d %.1f\n", l, C1);
		printf("%d %.1f\n", u, C2);
	}
	else printf("Invalid.");
	return 0;
}
10.
#include<stdio.h>
int main()
{
	int n = 0;
	scanf_s("%d", &n);
	double s = 0.0;
	
	for (int i = 1; i<=n;i++)
	{
		s += 1.0 / i;
	}
	printf("%.6lf\n", s);
	return 0;
}
11 题目意思理解有误
#include<stdio.h>
int main()
{
	int N = 0;
	scanf_s("%d", &N);
	if (N % 2 != 0)
	{
		int num = N / 2;
		float sum = 0;
		for (int i = 0; i < num; i++)
		{
			float a = 1.0 / N;
			N = N - 2;
			sum = sum + a;
		}
		float sum1 = sum + 1;
		printf("%.6f\n", sum1);
	}
	else if (N == 1)
		printf("1.000000");
	
	return 0;
}

#include<stdio.h>
int main()
{
	int N = 0;
	int d = 2;
	float sum = 0;
	scanf_s("%d", &N);
	for (int i = 0; i < N; i++)
	{
		float a = 1.0 /d;
		d = d + 2;
		sum = sum + a;
	}
	printf("%.6f\n", sum);
	return 0;
}
12.
//12
#include<stdio.h>
int main()
{
    int N = 0;
    scanf_s("%d", &N);
    float S = 0;
    float a = 0;
    int b = 1;
    int l = 1;
    for (int i = 1; i <= N; i++)
    {
        a = 1.0 / b*l;
        S = S + a;
        b =b + 3 ;
        l = -l ;
    }
    printf("sum = %.3f\n", S);
    return 0;
}
13
#include<stdio.h>
#include <math.h>
int main()
{
	int N = 0;
	scanf_s("%d", &N);
	for (int i = 0; i <= N; i++)
	{
	int res= pow( 3, i);
	printf("pow(3,%d)=%d\n", i, res);
	}
	return 0;
}
14
#include<stdio.h>
double fun(int n)
{
	int sum = 1;
	for (int i = 1; i < n; i++)
	{
		sum = sum * (i + 1);
	}
	return sum;
}
int main()
{
	int n = 0;
	int m = 0;
	scanf_s("%d %d", &m, &n);
	if (m > n)
		printf("输入值错误");
	double res = fun(n) / (fun(m) * fun(n - m ));
	printf("result = %.0lf", res);
}
#include<stdio.h>
int main()
{
	int a = 0;
	int b = 0;
	int c = 0;
	int d = 0;
	scanf_s("%d %d %d %d", &a, &b, &c, &d);
	float e =(a+b+c+d)/4.0;
	printf("Sum=%d Average=%.1f", a + b + c + d,e);
	return 0;
}
16.
#include<stdio.h>
int main()
{
	int use = 0;
	scanf_s("%d", &use);
	if (use <= 50)
		printf("cost=%.2f", use * 0.53);
	if (use > 50)
		printf("cost=%.2f", (use-50) * 0.58 + 50 * 0.53);
	return 0;
}

#include<stdio.h>
int main()
{
    int m,n;
    scanf("%d %d",&m,&n);
    int i;
    if(m<=n&&m!=0&&n!=0){
    double sum=0.0;
    for(i=m;i<=n;i++)
    {
        sum+=(double)pow(i,2)+1.0/i;
    }
    printf("sum = %.6lf\n",sum);}
    return 0;
}
18.
#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    int i;
    if(n!=0){
    double sum=0.0;
    int t1=1;
    int t2=1;
    int temp=1;
    for(i=1;i<=n;i++)
    {
        sum+=((double)t1/t2)*temp;
        t1++;
        t2+=2;
        temp=-temp;
    }
    printf("%.3lf\n",sum);}
    return 0;
}
